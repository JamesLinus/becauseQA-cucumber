   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            class="org.becausecucumber.eclipse.plugin.ui.launchs.CucumberRunFeatureLaunchShortcut"
            icon="icons/cukes.gif"
            id="org.becausecucumber.eclipse.plugin.launching.featureLaunchShortcut.run"
            label="Cucumber Runner"
            modes="run">
         <configurationType
               id="org.becausecucumber.eclipse.plugin.run">
         </configurationType>
         <contextualLaunch>
            <contextLabel
                  label="Cucumber Runner"
                  mode="run">
            </contextLabel>
            <enablement>
               <with
                     variable="selection">
               </with>
               <count
                     value="1">
               </count>
               <iterate>
                  <adapt
                        type="org.eclipse.core.resources.IResource">
                     <and>
                        <test
                              property="org.eclipse.core.resources.extension"
                              value="feature">
                        </test>
                     </and>
                  </adapt>
               </iterate>
            </enablement>
         </contextualLaunch>
      </shortcut>
