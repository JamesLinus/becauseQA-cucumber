        <markerResolutionGenerator
            class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
            markerType="org.becausecucumber.eclipse.plugin.ui.cucumber.check.normal">
            <attribute
                name="FIXABLE_KEY"
                value="true">
            </attribute>
        </markerResolutionGenerator>
        <markerResolutionGenerator
            class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
            markerType="org.becausecucumber.eclipse.plugin.ui.cucumber.check.expensive">
            <attribute
                name="FIXABLE_KEY"
                value="true">
            </attribute>
        </markerResolutionGenerator>
    </extension>
