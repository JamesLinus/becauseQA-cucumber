      <participant
            class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.eclipse.xtext.builder.IXtextBuilderParticipant"
            fileExtensions="feature"
            >
      </participant>
   </extension>
   <extension
            point="org.eclipse.ui.preferencePages">
        <page
            category="org.becausecucumber.eclipse.plugin.Cucumber"
            class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
            id="org.becausecucumber.eclipse.plugin.Cucumber.compiler.preferencePage"
