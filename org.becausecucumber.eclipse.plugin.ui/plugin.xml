<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>

    <extension
            point="org.eclipse.ui.editors">
        <editor
              class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.eclipse.xtext.ui.editor.XtextEditor"
              contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
              default="true"
              extensions="feature"
              icon="icons/cukes.gif"
              id="org.becausecucumber.eclipse.plugin.Cucumber"
              name="Cucumber Editor">
        </editor>
    </extension>
    <extension
        point="org.eclipse.ui.handlers">
        <handler
            class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
            commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
            <activeWhen>
                <reference
                    definitionId="org.becausecucumber.eclipse.plugin.Cucumber.Editor.opened">
                </reference>
            </activeWhen>
        </handler>
        <handler
            class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.becausecucumber.eclipse.plugin.ui.handler.ValidateActionHandler"
            commandId="org.becausecucumber.eclipse.plugin.Cucumber.validate">
         <activeWhen>
            <reference
                    definitionId="org.becausecucumber.eclipse.plugin.Cucumber.Editor.opened">
            </reference>
         </activeWhen>
      	</handler>
      	<!-- copy qualified name -->
        <handler
            class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
            commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
            <activeWhen>
				<reference definitionId="org.becausecucumber.eclipse.plugin.Cucumber.Editor.opened" />
            </activeWhen>
        </handler>
        <handler
            class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
            commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
            <activeWhen>
            	<and>
            		<reference definitionId="org.becausecucumber.eclipse.plugin.Cucumber.XtextEditor.opened" />
	                <iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
            </activeWhen>
        </handler>
    </extension>
    <extension point="org.eclipse.core.expressions.definitions">
        <definition id="org.becausecucumber.eclipse.plugin.Cucumber.Editor.opened">
            <and>
                <reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
                <with variable="activeEditor">
                    <test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
                        value="org.becausecucumber.eclipse.plugin.Cucumber" 
                        forcePluginActivation="true"/>
                </with>        
            </and>
        </definition>
        <definition id="org.becausecucumber.eclipse.plugin.Cucumber.XtextEditor.opened">
            <and>
                <reference definitionId="isXtextEditorActive"/>
                <with variable="activeEditor">
                    <test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
                        value="org.becausecucumber.eclipse.plugin.Cucumber" 
                        forcePluginActivation="true"/>
                </with>        
            </and>
        </definition>
    </extension>
    <extension
            point="org.eclipse.ui.preferencePages">
        <page
            class="org.becausecucumber.eclipse.plugin.ui.preference.CucumberMainPreferencePage"
            id="org.becausecucumber.eclipse.plugin.Cucumber"
            name="BecauseCucumber">
            <keywordReference id="org.becausecucumber.eclipse.plugin.ui.keyword_Cucumber"/>
        </page>
        <page
            category="org.becausecucumber.eclipse.plugin.Cucumber"
            class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.eclipse.xtext.ui.editor.syntaxcoloring.SyntaxColoringPreferencePage"
            id="org.becausecucumber.eclipse.plugin.Cucumber.coloring"
            name="Syntax Coloring">
            <keywordReference id="org.becausecucumber.eclipse.plugin.ui.keyword_Cucumber"/>
        </page>
        <page
              category="org.becausecucumber.eclipse.plugin.Cucumber"
              class="org.becausecucumber.eclipse.plugin.ui.preference.ContentAssitantPreferencePage"
              id="org.becausecucumber.eclipse.plugin.ui.contentassistantpage"
              name="Content Assist">
        </page>
        <page
            category="org.becausecucumber.eclipse.plugin.Cucumber"
            class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
            id="org.becausecucumber.eclipse.plugin.Cucumber.templates"
            name="Templates">
            <keywordReference id="org.becausecucumber.eclipse.plugin.ui.keyword_Cucumber"/>
        </page>
        <page
              category="org.becausecucumber.eclipse.plugin.Cucumber"
              class="org.becausecucumber.eclipse.plugin.ui.preference.AccountPreferencePage"
              id="org.becausecucumber.eclipse.plugin.Cucumber.accountsettings"
              name="Account Settings">
        </page>
        <page
              category="org.becausecucumber.eclipse.plugin.Cucumber"
              class="org.becausecucumber.eclipse.plugin.ui.preference.JenkinsPreferencePage"
              id="org.becausecucumber.eclipse.plugin.Cucumber.jenkins"
              name="Jenkins Integration">
        </page>
        <page
              category="org.becausecucumber.eclipse.plugin.Cucumber"
              class="org.becausecucumber.eclipse.plugin.ui.preference.RunResultPreferencePage"
              id="org.becausecucumber.eclipse.plugin.ui.run"
              name="Run/Result">
        </page>
    </extension>
    <extension
            point="org.eclipse.ui.propertyPages">
        <page
            class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
            id="org.becausecucumber.eclipse.plugin.Cucumber"
            name="Cucumber">
            <keywordReference id="org.becausecucumber.eclipse.plugin.ui.keyword_Cucumber"/>
            <enabledWhen>
	            <adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
	        <filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
        </page>
    </extension>
    <extension
        point="org.eclipse.ui.keywords">
        <keyword
            id="org.becausecucumber.eclipse.plugin.ui.keyword_Cucumber"
            label="Cucumber"/>
    </extension>
    <extension
         point="org.eclipse.ui.commands">
      <command
            description="Trigger expensive validation"
            id="org.becausecucumber.eclipse.plugin.Cucumber.validate"
            name="Cucumber Validate">
      </command>
      <!-- copy qualified name -->
      <command
            id="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
            categoryId="org.eclipse.ui.category.edit"
            description="Copy the qualified name for the selected element"
            name="Copy Qualified Name">
      </command>
      <command
            id="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
            categoryId="org.eclipse.ui.category.edit"
            description="Copy the qualified name for the selected element"
            name="Copy Qualified Name">
      </command>
    </extension>
    <extension point="org.eclipse.ui.menus">
        <menuContribution
            locationURI="popup:#TextEditorContext?after=group.edit">
             <command
                   commandId="org.becausecucumber.eclipse.plugin.Cucumber.validate"
                   icon="icons/cukes.gif"
                   style="push"
                   tooltip="Trigger expensive validation">
            <visibleWhen checkEnabled="false">
                <reference
                    definitionId="org.becausecucumber.eclipse.plugin.Cucumber.Editor.opened">
                </reference>
            </visibleWhen>
         </command>  
         </menuContribution>
         <!-- copy qualified name -->
         <menuContribution locationURI="popup:#TextEditorContext?after=copy">
         	<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName" 
         		style="push" tooltip="Copy Qualified Name">
            	<visibleWhen checkEnabled="false">
                	<reference definitionId="org.becausecucumber.eclipse.plugin.Cucumber.Editor.opened" />
            	</visibleWhen>
         	</command>  
         </menuContribution>
         <menuContribution locationURI="menu:edit?after=copy">
         	<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
            	style="push" tooltip="Copy Qualified Name">
            	<visibleWhen checkEnabled="false">
                	<reference definitionId="org.becausecucumber.eclipse.plugin.Cucumber.Editor.opened" />
            	</visibleWhen>
         	</command>  
         </menuContribution>
         <menuContribution locationURI="popup:org.eclipse.xtext.ui.outline?after=additions">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName" 
				style="push" tooltip="Copy Qualified Name">
         		<visibleWhen checkEnabled="false">
	            	<and>
	            		<reference definitionId="org.becausecucumber.eclipse.plugin.Cucumber.XtextEditor.opened" />
						<iterate>
							<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
						</iterate>
					</and>
				</visibleWhen>
			</command>
         </menuContribution>
    </extension>
    <extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?endof=group.find">
			<command commandId="org.eclipse.xtext.ui.editor.FindReferences">
				<visibleWhen checkEnabled="false">
                	<reference definitionId="org.becausecucumber.eclipse.plugin.Cucumber.Editor.opened">
                	</reference>
            	</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
	    <handler
            class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.eclipse.xtext.ui.editor.findrefs.FindReferencesHandler"
            commandId="org.eclipse.xtext.ui.editor.FindReferences">
            <activeWhen>
                <reference
                    definitionId="org.becausecucumber.eclipse.plugin.Cucumber.Editor.opened">
                </reference>
            </activeWhen>
        </handler>
    </extension>   

<!-- adding resource factories -->

	<extension
		point="org.eclipse.emf.ecore.extension_parser">
