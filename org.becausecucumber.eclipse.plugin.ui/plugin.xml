            point="org.eclipse.ui.ide.markerResolution">
        <markerResolutionGenerator
            class="org.becausecucumber.eclipse.plugin.ui.CucumberExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
